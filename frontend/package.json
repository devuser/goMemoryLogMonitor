{
  "name": "nemo-monitor-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx,.vue",
    "lint:fix": "eslint src --ext .ts,.tsx,.vue --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,vue,js,jsx,css,scss,html,json,md}\"",
